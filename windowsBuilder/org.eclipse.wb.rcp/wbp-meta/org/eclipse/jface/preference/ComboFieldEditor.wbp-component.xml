<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<description>A field editor for a combo box that allows the drop-down selection of one of a list of items.</description>
	<!-- CREATION -->
	<creation>
		<source><![CDATA[new org.eclipse.jface.preference.ComboFieldEditor("id", "New ComboFieldEditor", new String[][]{{"name_1", "value_1"}, {"name_2", "value_2"}}, %parentComposite%)]]></source>
	</creation>
	<!-- CONSTRUCTORS -->
	<constructors>
		<constructor>
			<parameter type="java.lang.String" property="setPreferenceName(java.lang.String)"/>
			<parameter type="java.lang.String" property="setLabelText(java.lang.String)"/>
			<parameter type="java.lang.String[][]" defaultSource="new String[][]{}">
				<editor id="FieldEditor_LabelsConstants"/>
			</parameter>
			<parameter type="org.eclipse.swt.widgets.Composite">
				<tag name="parentComposite" value="true"/>
			</parameter>
		</constructor>
	</constructors>
	<!-- PARAMETERS -->
	<parameters>
		<parameter name="FieldEditor.exposeMethods">getLabelControl</parameter>
	</parameters>
</component>
