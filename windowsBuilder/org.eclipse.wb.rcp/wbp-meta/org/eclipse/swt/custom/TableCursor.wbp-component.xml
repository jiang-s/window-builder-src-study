<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<model class="org.eclipse.wb.internal.rcp.model.widgets.TableCursorInfo"/>
	<description>A TableCursor provides a way for the user to navigate around a Table using the keyboard. It
		also provides a mechanism for selecting an individual cell in a table.</description>
	<!-- CREATION -->
	<creation>
		<source><![CDATA[new org.eclipse.swt.custom.TableCursor(%parent%, org.eclipse.swt.SWT.NONE)]]></source>
	</creation>
	<!-- CONSTRUCTORS -->
	<constructors>
		<constructor>
			<parameter type="org.eclipse.swt.widgets.Table" parent="true"/>
			<parameter type="int" defaultSource="org.eclipse.swt.SWT.NONE">
				<editor id="style">
					<parameter name="class">org.eclipse.swt.SWT</parameter>
					<parameter name="set">BORDER</parameter>
				</editor>
			</parameter>
		</constructor>
	</constructors>
	<!-- PARAMETERS -->
	<parameters>
		<parameter name="layout.has">false</parameter>
	</parameters>
</component>
