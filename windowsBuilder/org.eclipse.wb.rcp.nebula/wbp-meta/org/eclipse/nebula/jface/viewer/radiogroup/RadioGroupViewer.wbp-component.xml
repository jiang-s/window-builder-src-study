<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<description>A concrete viewer based on a Nebula RadioGroup control.</description>
	<!-- CREATION -->
	<creation>
		<source><![CDATA[new org.eclipse.nebula.jface.viewer.radiogroup.RadioGroupViewer(%parent%, org.eclipse.swt.SWT.BORDER)]]></source>
	</creation>
	<!-- CONSTRUCTORS -->
	<constructors>
		<constructor>
			<parameter type="org.eclipse.swt.widgets.Composite" parent="true"/>
			<parameter type="int" defaultSource="org.eclipse.swt.SWT.NONE">
				<editor id="style">
					<parameter name="class">org.eclipse.swt.SWT</parameter>
					<parameter name="set">BORDER</parameter>
					<parameter name="select0">align LEFT LEFT RIGHT CENTER</parameter>
					<parameter name="select1">orientation HORIZONTAL HORIZONTAL VERTICAL</parameter>
					<parameter name="select2">direction LEFT_TO_RIGHT LEFT_TO_RIGHT RIGHT_TO_LEFT</parameter>
				</editor>
			</parameter>
		</constructor>
		<constructor>
			<parameter type="org.eclipse.swt.widgets.Composite" parent="true"/>
		</constructor>
		<constructor>
			<parameter type="org.eclipse.nebula.widgets.radiogroup.RadioGroup" parent="true"/>
		</constructor>
	</constructors>
	<!-- EXPOSING RULES -->
	<exposing-rules>
		<exclude package="org.eclipse.nebula.jface.viewer.radiogroup"/>
	</exposing-rules>
	<!-- PARAMETERS -->
	<parameters>
		<parameter name="viewer.control.method">getRadioGroup</parameter>
	</parameters>
</component>
