<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<description>Instances of the receiver represent is an unselectable user interface object that is used to
		display progress, typically in the form of a bar.</description>
	<!-- CREATION -->
	<creation>
		<source><![CDATA[new org.eclipse.swt.widgets.ProgressBar(%parent%, org.eclipse.swt.SWT.NONE)]]></source>
	</creation>
	<!-- CONSTRUCTORS -->
	<constructors>
		<constructor>
			<parameter type="org.eclipse.swt.widgets.Composite" parent="true"/>
			<parameter type="int" defaultSource="org.eclipse.swt.SWT.NONE">
				<editor id="style">
					<parameter name="class">org.eclipse.swt.SWT</parameter>
					<parameter name="set">BORDER SMOOTH INDETERMINATE:unknown</parameter>
					<parameter name="select0">dir HORIZONTAL HORIZONTAL VERTICAL</parameter>
				</editor>
			</parameter>
		</constructor>
	</constructors>
	<!-- METHODS -->
	<methods>
		<method name="setMinimum" order="after setMaximum(int)">
			<parameter type="int"/>
		</method>
		<method name="setSelection" order="after setMinimum(int)">
			<parameter type="int"/>
		</method>
	</methods>
	<!-- PROPERTIES -->
	<properties-preferred names="minimum maximum selection"/>
	<property id="setState(int)">
		<editor id="staticField">
			<parameter name="class">org.eclipse.swt.SWT</parameter>
			<parameter name="fields">NORMAL ERROR PAUSED</parameter>
		</editor>
	</property>
</component>