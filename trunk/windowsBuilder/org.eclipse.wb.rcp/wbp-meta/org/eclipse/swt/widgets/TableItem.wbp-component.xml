<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<model class="org.eclipse.wb.internal.swt.model.widgets.TableItemInfo"/>
	<x-model class="org.eclipse.wb.internal.xwt.model.widgets.TableItemInfo"/>
	<description>Instances of this class represent a selectable user interface object that represents an item
		in a table.</description>
	<!-- CREATION -->
	<creation>
		<source><![CDATA[new org.eclipse.swt.widgets.TableItem(%parent%, org.eclipse.swt.SWT.NONE)]]></source>
		<invocation signature="setText(java.lang.String)"><![CDATA["New TableItem"]]></invocation>
		<x-attribute name="text" value="New TableItem"/>
	</creation>
	<!-- CONSTRUCTORS -->
	<constructors>
		<constructor>
			<parameter type="org.eclipse.swt.widgets.Table" parent="true"/>
			<parameter type="int">
				<editor id="displayExpression"/>
			</parameter>
		</constructor>
		<constructor>
			<parameter type="org.eclipse.swt.widgets.Table" parent="true"/>
			<parameter type="int">
				<editor id="displayExpression"/>
			</parameter>
			<parameter type="int"/>
		</constructor>
	</constructors>
	<!-- METHODS -->
	<methods>
		<!-- COLOR -->
		<method name="setBackground">
			<parameter type="int"/>
			<parameter type="org.eclipse.swt.graphics.Color"/>
		</method>
		<method name="setForeground">
			<parameter type="int"/>
			<parameter type="org.eclipse.swt.graphics.Color"/>
		</method>
		<!-- FONT -->
		<method name="setFont">
			<parameter type="int"/>
			<parameter type="org.eclipse.swt.graphics.Font"/>
		</method>
		<method name="setFont">
			<parameter type="int"/>
			<parameter type="org.eclipse.swt.graphics.Font"/>
		</method>
		<!-- IMAGE -->
		<method name="setImage">
			<parameter type="org.eclipse.swt.graphics.Image[]"/>
		</method>
		<method name="setImage">
			<parameter type="int"/>
			<parameter type="org.eclipse.swt.graphics.Image"/>
		</method>
		<!-- TEXT -->
		<method name="setText">
			<parameter type="java.lang.String[]"/>
		</method>
		<method name="setText">
			<parameter type="int"/>
			<parameter type="java.lang.String"/>
		</method>
	</methods>
	<!-- PROPERTIES -->
	<properties-preferred names="text image checked grayed"/>
	<properties-advanced names="imageIndent"/>
</component>
