	<creation>
		<source><![CDATA[new org.eclipse.jface.viewers.ColumnWeightData(1, org.eclipse.jface.viewers.ColumnWeightData.MINIMUM_WIDTH, true)]]></source>
	</creation>
	<!-- CONSTRUCTORS -->
	<constructors>
		<constructor>
			<parameter type="int" property="weight"/>
		</constructor>
		<constructor>
			<parameter type="int" property="weight"/>
			<parameter type="int" property="minimumWidth"
				defaultSource="org.eclipse.jface.viewers.ColumnWeightData.MINIMUM_WIDTH"/>
		</constructor>
		<constructor>
			<parameter type="int" property="weight"/>
			<parameter type="int" property="minimumWidth"
				defaultSource="org.eclipse.jface.viewers.ColumnWeightData.MINIMUM_WIDTH"/>
			<parameter type="boolean" property="resizable"/>
		</constructor>
		<constructor>
			<parameter type="int" property="weight"/>
			<parameter type="boolean" property="resizable"/>
		</constructor>
	</constructors>
</component>
