<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<description>A field editor to edit directory paths.</description>
	<!-- CREATION -->
	<creation>
		<source><![CDATA[new org.eclipse.jface.preference.PathEditor("id", "New PathEditor", "Select directory:", %parentComposite%)]]></source>
	</creation>
	<!-- CONSTRUCTORS -->
	<constructors>
		<constructor>
			<parameter type="java.lang.String" property="setPreferenceName(java.lang.String)"/>
			<parameter type="java.lang.String" property="setLabelText(java.lang.String)"/>
			<parameter type="java.lang.String"/>
			<parameter type="org.eclipse.swt.widgets.Composite">
				<tag name="parentComposite" value="true"/>
			</parameter>
		</constructor>
	</constructors>
</component>
