<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<model class="org.eclipse.wb.internal.ercp.model.widgets.mobile.ListBoxItemInfo"/>
	<description>Instances of this class represent an item in a ListBox widget. Heading and detail icons, if
		provided, are displayed at the size provided, or stretched to fit the ListBox styles.</description>
	<!-- CREATION -->
	<creation>
		<source><![CDATA[new org.eclipse.ercp.swt.mobile.ListBoxItem("detail #%index%", null, "heading #%index%", null)]]></source>
	</creation>
	<!-- CONSTRUCTORS -->
	<constructors>
		<constructor>
			<parameter type="java.lang.String" property="setDetailText(java.lang.String)"/>
			<parameter type="org.eclipse.swt.graphics.Image"/>
			<parameter type="java.lang.String" property="setHeadingText(java.lang.String)"/>
			<parameter type="org.eclipse.swt.graphics.Image"/>
		</constructor>
	</constructors>
	<!-- PROPERTIES -->
	<properties-preferred names="detailText headingText"/>
	<property-tag name="detailText" tag="isText" value="true"/>
</component>
